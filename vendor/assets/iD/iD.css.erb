    fill: #eee;
}
.flash-icon.disabled circle {
    cursor: auto;
    fill: rgba(255,255,255,0.7);
}

.flash-icon use {
    color: #222;
}
.flash-icon.disabled use {
    color: rgba(32,32,32,0.7);
}

