    flex-flow: row nowrap;
    justify-content: space-between;
    max-height: 30px;
    position: absolute;
    right: 0;
    left: 0;
}

.footer-show {
    bottom: 0px;
    transition: bottom 75ms linear;
    -moz-transition: bottom 75ms linear;
    -webkit-transition: bottom 75ms linear;
}

.footer-hide {
    bottom: -35px;
    transition: bottom 75ms linear;
    -moz-transition: bottom 75ms linear;
    -webkit-transition: bottom 75ms linear;
