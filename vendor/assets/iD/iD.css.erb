/* Notices (Zoom in to Edit)
------------------------------------------------------- */
.notice {
    position: absolute;
    top: 45px;
    left: 0;
    right: 0;
    text-align: center;
}

.notice .zoom-to {
    margin: auto;
    width: 300px;
